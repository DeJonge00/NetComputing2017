package task_distributor;

import java.rmi.Naming;
import java.rmi.RMISecurityManager;

import rmi.TaskManager;

public class TaskDistributor {
	  
	public TaskDistributor() {
		try {
			System.out.println("Starting task distributor");
			System.setSecurityManager(new RMISecurityManager());
			TaskManager stub=(TaskManager)Naming.lookup("rmi://localhost:1099/taskManager");  
			System.out.println(stub);
			System.out.println(stub.execute("test"));  
		}catch(Exception e){}   
	}  
}
