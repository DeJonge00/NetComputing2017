package rmi;

public class TaskInfo {
	private int pid;
	private String status;
	
	public TaskInfo(int pid, String s) {
		this.pid = pid;
		this.status = status;
	}

	public int getPid() {
		return pid;
	}

	public void setPid(int pid) {
		this.pid = pid;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}
}
